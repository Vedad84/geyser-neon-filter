{
    "http_settings": {
        "connect_timeout": "5s",
        "keepalive": "60s",
        "pool_idle_timeout": "5s",
        "no_delay": true,
        "reuse_address": true,
        "retries": 3,
        "retry_delay": "1s"
    },
    "log_level": "Info",
    "log_path": "/var/log/neon/scheduler.log",
    "servers": [
        "http://127.0.0.1:8123"
    ],
    "username": "",
    "password": "",
    "tasks": [
        {
            "task_name": "two_multiply_queries",
            "queries": [
                "SELECT 1*1;",
                "SELECT 2*1;"
            ],
            "cron": "0 0/2 * 1/1 * ? *",
            "average_depth": 30,
            "select_execute_result": false
        },
        {
            "task_name": "three_sum_queries",
            "queries": [
                "SELECT 1+1;",
                "SELECT 2+1;",
                "SELECT 3+1;"
            ],
            "cron": "0 0/2 * 1/1 * ? *",
            "average_depth": 30,
            "select_execute_result": false
        },
        {
            "task_name": "cron_one_query",
            "queries": [
                "SELECT 1+1;"
            ],
            "cron": "0 0/1 * 1/1 * ? *",
            "average_depth": 30,
            "select_execute_result": false
        },
        {
            "task_name": "optimize_table_update_account",
            "queries": [
                "SELECT CONCAT('OPTIMIZE TABLE events.', table, ' ON CLUSTER events PARTITION ', partition_id) FROM system.parts WHERE (database = 'events') AND (table = 'update_account_local') GROUP BY table, partition_id ORDER BY table, partition_id DESC LIMIT 1, 1"
            ],
            "cron": "0 0 1 * * ?",
            "average_depth": 30,
            "select_execute_result": true
        },
        {
            "task_name": "optimize_table_notify_transaction",
            "queries": [
                "SELECT CONCAT('OPTIMIZE TABLE events.', table, ' ON CLUSTER events PARTITION ', partition_id) FROM system.parts WHERE (database = 'events') AND (table = 'notify_transaction_local') GROUP BY table, partition_id ORDER BY table, partition_id DESC LIMIT 1, 1"
            ],
            "cron": "0 0 1 * * ?",
            "average_depth": 30,
            "select_execute_result": true
        },
        {
            "task_name": "optimize_table_notify_block",
            "queries": [
                "SELECT CONCAT('OPTIMIZE TABLE events.', table, ' ON CLUSTER events PARTITION ', partition_id) FROM system.parts WHERE (database = 'events') AND (table = 'notify_block_local') GROUP BY table, partition_id ORDER BY table, partition_id DESC LIMIT 1, 1"
            ],
            "cron": "0 0 1 * * ?",
            "average_depth": 30,
            "select_execute_result": true
        }
    ],
    "prometheus_port": "9090"
}
