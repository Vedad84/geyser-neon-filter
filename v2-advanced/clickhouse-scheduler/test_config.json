{
    "http_settings": {
        "connect_timeout": "5s",
        "keepalive": "60s",
        "pool_idle_timeout": "5s",
        "no_delay": true,
        "reuse_address": true,
        "retries": 3,
        "retry_delay": "1s"
    },
    "log_level": "Info",
    "log_path": "/var/log/neon/scheduler.log",
    "servers": [
        "http://10.1.0.2:8123",
        "http://10.1.0.3:8123"
    ],
    "username": "",
    "password": "",
    "tasks": [
        {
            "task_name": "two_multiply_queries",
            "queries": [
                "SELECT 1*1;",
                "SELECT 2*1;"
            ],
            "cron": "0 0/2 * 1/1 * ? *",
            "average_depth": 30
        },
        {
            "task_name": "three_sum_queries",
            "queries": [
                "SELECT 1+1;",
                "SELECT 2+1;",
                "SELECT 3+1;"
            ],
            "cron": "0 0/2 * 1/1 * ? *",
            "average_depth": 30
        },
        {
            "task_name": "cron_one_query",
            "queries": [
                "SELECT 1+1;"
            ],
            "cron": "0 0/1 * 1/1 * ? *",
            "average_depth": 30
        }
    ],
    "prometheus_port": "9090"
}
